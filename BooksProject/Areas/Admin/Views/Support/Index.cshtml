@{
    ViewData["Title"] = "Support Page";
}
<div class="container mt-3">
    <div class="row">
        <div class="col-9">
            <h2 class="text-info">Support List</h2>
        </div>
        <div class="col-3">
            <a asp-action="Upsert" class="btn btn-info form-control">
                <i class="fa fa-plus"></i>&nbsp; Add New Question
            </a>
        </div>
    </div>
    <div class="container rounded border p-2 m-2">
        <table id="tblData" class="table table-striped table-bordered table-hover">
            <thead class="bg-info">
                <tr>
                    <th class="text-light">Edit</th>
                    <th class="text-light">Question</th>
                    <th class="text-light">Answer</th>
                    <th class="text-light">Delete</th>
                </tr>
            </thead>
        </table>
    </div>
</div>
@section Scripts  {
    <script src="~/js/Support.js"></script>
}